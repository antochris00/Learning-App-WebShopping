<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ui.admin" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>AdminUI</name>
    <description>
        Admin UI - angular to android application for admin functionality
    </description>
    <author email="muhilkennedy@gmail.com" href="http://cordova.io">
        Muhil
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon background="src/assets/img/brand/Brand-Icon.png" density="ldpi" foreground="src/assets/img/brand/Brand-Icon.png" />
    <allow-intent href="market:*" />
    <config-file mode="merge" parent="/*" target="AndroidManifest.xml">
        <uses-permission android:name="android.permission.CAMERA" />
        <uses-feature android:name="android.hardware.camera" />
        <uses-feature android:name="android.hardware.camera.autofocus" />
    </config-file>
    <preference name="loglevel" value="DEBUG" />
</widget>
